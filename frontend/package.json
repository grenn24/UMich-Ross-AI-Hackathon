{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "set VITE_ENV=production&& vite --mode production",
		"dev": "set VITE_ENV=development&& vite --mode development",
		"build": "set VITE_ENV=production&& npx tsc -b && vite build --mode production",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"preview": "vite preview",
		"format": "npx prettier --check \"src/**/*.{js,ts,jsx,tsx}\"",
		"format:fix": "npx prettier --write \"src/**/*.{js,ts,jsx,tsx}\""
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.5.1",
		"axios": "^1.7.9",
		"config": "^3.3.12",
		"locale-currency": "^0.0.4",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.1.5",
		"redux": "^5.0.1",
		"redux-persist": "^6.0.0",
		"uuid": "^11.1.0",
		"vite-plugin-svgr": "^4.5.0",
		"vite-tsconfig-paths": "^5.1.4",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@types/config": "^3.3.5",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@types/webfontloader": "^1.6.38",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.19.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"typescript": "~5.7.3",
		"typescript-eslint": "^8.22.0",
		"vite": "^6.0.11"
	}
}
